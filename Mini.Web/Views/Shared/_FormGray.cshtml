<!--Areas文件夹下表单页面灰色背景模板-->
@inject Microsoft.AspNetCore.Hosting.IWebHostEnvironment HostingEnvironment
<!DOCTYPE HTML>
<html>
<head>
    <meta charset="utf-8">
    <meta name="renderer" content="webkit|ie-comp|ie-stand">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width,initial-scale=1,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" />
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <script type="text/javascript" src='@Url.Content("~/lib/jquery/2.1.4/jquery.min.js")'></script>
    <link href='@Url.Content("~/customize/toastr/toastr.css")' rel="stylesheet" />
    <script src='@Url.Content("~/customize/toastr/toastr.min.js")'></script>
    <script src='@Url.Content("~/customize/js/pack.js")'></script>
    @await Html.PartialAsync("_Form.cshtml")

    @RenderSection("header", false)
    <script type="text/javascript">
        var ctx = '@Url.Content("~/")';
    </script>
</head>
<body class="gray-bg">
    @RenderBody()
    @RenderSection("footer", false)
    <link href='@Url.Content("~/customize/css/yisha.css")' rel="stylesheet" />
    <script type="text/javascript" src='@Url.Content("~/customize/js/init.min.js")'></script>
</body>
</html>